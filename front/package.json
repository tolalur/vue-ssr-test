{
  "name": "vue-ssr-front",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "nodemon": "nodemon",
    "dev-client": "webpack serve --config ./config/webpack.client.js",
    "dev-server": "cross-env IS_SERVER=true webpack -w --config ./config/webpack.server.js",
    "dev": "run-p dev-client dev-server nodemon",
    "clean": "rimraf ./dist/",
    "build-client": "cross-env NODE_ENV=production webpack --config ./config/webpack.client.js",
    "build-server": "cross-env  IS_SERVER=true NODE_ENV=production webpack --config ./config/webpack.server.js",
    "build-all": "run-p build-client build-server",
    "build": "run-s clean build-all",
    "start": "node src/server.js",
    "format": "prettier --write \"src/**/*.ts\" \"*.js\" \"src/**/*.vue\""
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@vue/composition-api": "^1.2.2",
    "axios": "^0.21.4",
    "express": "^4.17.1",
    "http-proxy": "^1.18.1",
    "vue": "^2.6.14",
    "vue-router": "^3.5.2",
    "vue-server-renderer": "^2.6.14"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-typescript": "^7.15.0",
    "@types/express": "^4.17.13",
    "@types/node": "^16.9.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.2.2",
    "babel-preset-typescript-vue": "^1.1.1",
    "clean-webpack-plugin": "^4.0.0",
    "cross-env": "^7.0.3",
    "css-minimizer-webpack-plugin": "^3.0.2",
    "fork-ts-checker-webpack-plugin": "^6.3.3",
    "html-webpack-plugin": "^5.3.2",
    "mini-css-extract-plugin": "^2.3.0",
    "node-sass": "^6.0.1",
    "nodemon": "^2.0.12",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "sass-loader": "^12.1.0",
    "thread-loader": "^3.0.4",
    "ts-loader": "^9.2.6",
    "typescript": "^4.4.3",
    "url-loader": "^4.1.1",
    "vue-loader": "^15.9.8",
    "vue-style-loader": "^4.1.3",
    "vue-template-compiler": "^2.6.14",
    "webpack": "^5.53.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.2.1",
    "webpack-merge": "^5.8.0",
    "webpack-node-externals": "^3.0.0"
  }
}
