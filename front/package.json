{
  "name": "vue-ssr-front",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "nodemon": "nodemon",
    "dev-client": "webpack serve --config ./config/webpack.client.js",
    "dev-server": "cross-env IS_SERVER=true webpack -w --config ./config/webpack.server.js",
    "dev": "run-p dev-client dev-server nodemon",
    "clean": "rimraf ./dist/",
    "build-client": "cross-env NODE_ENV=production webpack --config ./config/webpack.client.js",
    "build-server": "cross-env  IS_SERVER=true NODE_ENV=production webpack --config ./config/webpack.server.js",
    "build-all": "run-p build-client build-server",
    "build": "run-s clean build-all",
    "start": "node ssr/server.js",
    "lint": "eslint  \"src/**/*.ts\" \"*.js\" \"src/**/*.vue\" --fix"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.21.4",
    "express": "^4.17.1",
    "http-proxy": "^1.18.1",
    "vue": "^3.2.19",
    "vue-router": "^4.0.11",
    "vue-server-renderer": "^2.6.14"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.6",
    "@types/express": "^4.17.13",
    "@types/node": "^16.9.1",
    "@vue/compiler-sfc": "^3.2.19",
    "@vue/eslint-config-typescript": "^7.0.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.2.2",
    "cross-env": "^7.0.3",
    "css-loader": "^6.3.0",
    "css-minimizer-webpack-plugin": "^3.0.2",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^7.19.1",
    "fork-ts-checker-webpack-plugin": "^6.3.3",
    "mini-css-extract-plugin": "^2.3.0",
    "node-sass": "^6.0.1",
    "nodemon": "^2.0.12",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "sass-loader": "^12.1.0",
    "serialize-javascript": "^3.1.0",
    "serve-favicon": "^2.5.0",
    "thread-loader": "^3.0.4",
    "ts-loader": "^9.2.6",
    "typescript": "^4.4.3",
    "url-loader": "^4.1.1",
    "vue-bundle-renderer": "^0.3.1",
    "vue-loader": "^16.8.1",
    "vue-style-loader": "^4.1.3",
    "webpack": "^5.53.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.3.1",
    "webpack-merge": "^5.8.0",
    "webpack-node-externals": "^3.0.0"
  }
}
